# OpenFile gate.wlf
add wave -position insertpoint  \
gate:/alu/in_a \
gate:/alu/in_b \
gate:/alu/opcode \
gate:/alu/alu_out \
gate:/alu/alu_zero \
gate:/alu/alu_carry
